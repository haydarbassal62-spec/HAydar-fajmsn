import requests # ڪود إشتࢪآڪہ عبࢪ IP الهآتف
rres = requests.get('https://api.myip.com').json()
ipp1 = rres['ip']
ipp2 = '' # هنا IP المشتࢪڪہ
if ipp1 == ipp2:
 print('')
 pass
else:
 exit('''\033[1;31m
 EROR - عزيزي أنت غيࢪ مشتࢪڪہ
 \033[2;32m
 TG - @ql_37 للإشتࢪآڪہ ࢪآسل ''')
import datetime
an = datetime.datetime.now()
an2 = datetime.datetime(2025, 11, 29, 11, 59, 59)
if an > an2 or an == an2:
    exit('''\033[1;31m
 EROR - عزيزي أنت غيࢪ مشتࢪڪہ
 \033[2;32m
 TG - @ql_37 للإشتࢪآڪہ ࢪآسل ''')
